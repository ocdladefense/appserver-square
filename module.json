{
	"comment": "Module definition file.",
	"name": "square",
	"description": "OCDLA payments.",
	"files": [
		"Customer.php",
		"Order.php",
		"ShoppingCart.php",
		"SquarePayment.php",
        "SquareCustomer.php",
        "IPaymentProcessor.php",
        "CartTemplate.php"
	],
	"routes": {
		"order/%orderId":{
			"callback":"OrderPreview",
			"content-type": "text\/html"
		},
		"order":{
			"callback":"OrderPreview",
			"content-type": "text\/html"
		},
		"cart/deleteItem/%cartItem":{
			"callback":"DeleteCartItem",
			"content-type":"application\/json"
		},
		"checkout/setInactiveCard/%cardId":{
			"callback":"setInactiveCard",
			"content-type":"application\/json"
		},
		"checkout/deleteCard/%cardId":{
			"callback":"deleteCard",
			"content-type":"application\/json"
		}
    }
}